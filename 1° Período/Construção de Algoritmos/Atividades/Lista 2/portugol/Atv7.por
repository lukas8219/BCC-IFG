programa {
  funcao inicio() 
  {
      real conta, felipe
      inteiro carlos, andre

      escreva("Conta do restaurante: ")
      leia(conta)

      carlos = conta/3
      andre = conta/3
      felipe = conta - (carlos + andre)

      escreva("Carlos: ", carlos, "\nAndré: ", andre, "\nFelipe: ", felipe)

  }
}
