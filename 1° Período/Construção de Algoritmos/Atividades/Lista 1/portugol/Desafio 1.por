programa {
  funcao inicio() 
  {
      inteiro m, n, o, p, q, soma

      escreva("m: ")
      leia(m)
      escreva("n: ")
      leia(n)
      escreva("o: ")
      leia(o)
      escreva("p: ")
      leia(p
      escreva("q: ")
      leia(q)

      m=m*(m%2)
      n=n*(n%2)
      o=o*(o%2)
      p=p*(p%2)
      q=q*(q%2)

      soma=m+n+o+p+q
      escreva("A soma dos números ímpares é de: ", soma)
  }
}
